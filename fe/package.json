	"scripts": {
		"build": "vite build",
		"chmod": "chmod u+x src/bin/*",
		"dev": "vite dev",
		"linksvizzles": "npm install && npm link @svizzle/dev @svizzle/dom @svizzle/histogram @svizzle/legend @svizzle/ui @svizzle/utils && npm list",
		"lint": "eslint .",
		"makeData": "npm run makeHierarchy && npm run makeRegions",
		"makeHierarchy": "node src/bin/regions/makeHierarchy.js",
		"makeRegions": "node src/bin/regions/makeRegions.js",
		"preview": "vite preview",
		"redev": "npm run rmkv && npm run dev",
		"reinstall": "npm run uninstall && npm install",
		"rmkit": "rimraf .svelte-kit",
		"rmkv": "npm run rmkit && npm run rmvite",
		"rmvite": "rimraf node_modules/.vite",
		"uninstall": "rimraf node_modules package-lock.json",
		"unittest": "mocha --spec src/**/*.spec.js"
